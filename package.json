{
  "name": "fil",
  "version": "8.2.3",
  "description": "A superfast static site generator",
  "scripts": {
    "compile": "shx rm -r compiled && cross-env NODE_ENV=compile babel app -d compiled",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "cross-env NODE_ENV=test nyc --all spec/run.js",
    "test:debug": "cross-env NODE_ENV=test node $NODE_DEBUG_OPTION spec/run.js"
  },
  "bin": {
    "fil": "./compiled/bin/index.js"
  },
  "author": "Umut Benzer <umut@benzer.me>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.23.6",
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "gauge": "^2.7.4",
    "lodash.isplainobject": "^4.0.6",
    "mime-types": "^2.1.18",
    "minimist": "^1.2.0",
    "npid": "^0.4.0",
    "rxjs": "^5.5.7",
    "supports-color": "^5.3.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.18.2",
    "jasmine": "^3.1.0",
    "jasmine-promise-wrapper": "0.0.3",
    "nyc": "^11.6.0",
    "shx": "^0.2.2"
  },
  "keywords": [
    "static-site",
    "jekyll",
    "hexo",
    "blog",
    "wordpress",
    "collections",
    "taxonomy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubenzer/fil.git"
  },
  "bugs": {
    "url": "https://github.com/ubenzer/fil/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "include": [
      "app/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "json"
    ]
  }
}
